{
  "name": "UPS Label App",
  "author": {
    "name": "Zendesk Services / Rose Trujillo",
    "email": "services@zendesk.com"
  },
  "defaultLocale": "en",
  "private": true,
  "location": [ "new_ticket_sidebar", "ticket_sidebar" ],
  "version": "1.0",
  "frameworkVersion": "1.0",

  "parameters": [
    {
      "name": "company_name",
      "type": "text",
      "default": "Support"
    },
    {
      "name": "business_address",
      "type": "text",
      "default": "989 Market Street"
    },
    {
      "name": "city",
      "type": "text",
      "default": "San Francisco"
    },
    {
      "name": "state",
      "type": "text",
      "default": "CA"
    },
    {
      "name": "country_code",
      "type": "text",
      "default": "US"
    },    
    {
      "name": "zip_code",
      "type": "number",
      "default": "94103"
    },
    {
      "name": "support_email",
      "type": "text",
      "default": "support@example.com"
    },
    {
      "name": "editable_form",
      "type": "checkbox",
      "default": "no"
    },
    {
      "name": "user_address_field",
      "type": "text",
      "default": "address"
    },
    {
      "name": "user_city_field",
      "type": "text",
      "default": "city"
    },
    {
      "name": "user_state_field",
      "type": "text",
      "default": "state"
    },
    {
      "name": "user_zip_field",
      "type": "text",
      "default": "Zip Code"
    },
    {
      "name": "user_country_field",
      "type": "text",
      "default": "country"
    },
    {
      "name": "small_size_width",
      "type": "text",
      "default": "3"
    },
    {
      "name": "small_size_height",
      "type": "text",
      "default": "5"
    },
    {
      "name": "small_size_length",
      "type": "text",
      "default": "5"
    },
    {
      "name": "small_size_weight",
      "type": "text",
      "default": "5"
    },
    {
      "name": "medium_size_width",
      "type": "text",
      "default": "5"
    },
    {
      "name": "medium_size_height",
      "type": "text",
      "default": "7"
    },
    {
      "name": "medium_size_length",
      "type": "text",
      "default": "7"
    },
    {
      "name": "medium_size_weight",
      "type": "text",
      "default": "10"
    },
    {
      "name": "large_size_width",
      "type": "text",
      "default": "8"
    },
    {
      "name": "large_size_height",
      "type": "text",
      "default": "10"
    },
    {
      "name": "large_size_length",
      "type": "text",
      "default": "10"
    },
    {
      "name": "large_size_weight",
      "type": "text",
      "default": "15"
    },
    {
      "name": "ups_username",
      "type": "text",
      "default": "15"
    },
    {
      "name": "ups_password",
      "type": "text",
      "default": "15"
    },
    {
      "name": "ups_access_number",
      "type": "text",
      "default": "15"
    },
    {
      "name": "tracking_field",
      "type": "number",
      "default": 21947252
    }
  ]
}
