{
  "name": "UPS Label App",
  "author": {
    "name": "Zendesk Services / Rose Trujillo",
    "email": "services@zendesk.com"
  },
  "defaultLocale": "en",
  "private": true,
  "location": [ "new_ticket_sidebar", "ticket_sidebar" ],
  "version": "1.0",
  "frameworkVersion": "1.0",

  "parameters": [
    {
      "name": "company_name",
      "type": "text",
      "default": "Support"
    },
    {
      "name": "business_address",
      "type": "text",
      "default": "989 Market Street"
    },
    {
      "name": "city",
      "type": "text",
      "default": "San Francisco"
    },
    {
      "name": "state",
      "type": "text",
      "default": "CA"
    },
    {
      "name": "zip_code",
      "type": "number",
      "default": "94103"
    },
    {
      "name": "support_email",
      "type": "text",
      "default": "support@example.com"
    }
  ]
}
