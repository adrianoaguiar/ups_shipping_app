{
  "name": "UPS Label App",
  "author": {
    "name": "Zendesk Services / Rose Trujillo",
    "email": "services@zendesk.com"
  },
  "defaultLocale": "en",
  "private": true,
  "location": [ "ticket_sidebar" ],
  "domainWhitelst": [ "https://onlinetools.ups.com", "https://wwwcie.ups.com" ],
  "version": "1.2",
  "frameworkVersion": "1.0",

  "parameters": [
    {
      "name": "company_name",
      "type": "text",
      "default": "Support",
      "required": true
    },
    {
      "name": "attention_name",
      "type": "text",
      "required": true
    },
    {
      "name": "business_address",
      "type": "text",
      "default": "989 Market Street",
      "required": true
    },
    {
      "name": "city",
      "type": "text",
      "default": "San Francisco",
      "required": true
    },
    {
      "name": "state",
      "type": "text",
      "default": "CA",
      "required": true
    },
    {
      "name": "country_code",
      "type": "text",
      "default": "US",
      "required": true
    },
    {
      "name": "zip_code",
      "type": "number",
      "default": "94103",
      "required": true
    },
    {
      "name": "support_email",
      "type": "text",
      "default": "support@example.com",
      "required": true
    },
    {
      "name": "phone_number",
      "type": "number",
      "required": true
    },
    {
      "name": "editable_form",
      "type": "checkbox",
      "default": "no"
    },
    {
      "name": "user_address_field",
      "type": "text",
      "default": "address"
    },
    {
      "name": "user_city_field",
      "type": "text",
      "default": "city"
    },
    {
      "name": "user_state_field",
      "type": "text",
      "default": "state"
    },
    {
      "name": "user_zip_field",
      "type": "text",
      "default": "Zip Code"
    },
    {
      "name": "user_country_field",
      "type": "text",
      "default": "country"
    },
    {
      "name": "small_size_width",
      "type": "text",
      "default": "3"
    },
    {
      "name": "small_size_height",
      "type": "text",
      "default": "5"
    },
    {
      "name": "small_size_length",
      "type": "text",
      "default": "5"
    },
    {
      "name": "small_size_weight",
      "type": "text",
      "default": "5"
    },
    {
      "name": "medium_size_width",
      "type": "text",
      "default": "5"
    },
    {
      "name": "medium_size_height",
      "type": "text",
      "default": "7"
    },
    {
      "name": "medium_size_length",
      "type": "text",
      "default": "7"
    },
    {
      "name": "medium_size_weight",
      "type": "text",
      "default": "10"
    },
    {
      "name": "large_size_width",
      "type": "text",
      "default": "8"
    },
    {
      "name": "large_size_height",
      "type": "text",
      "default": "10"
    },
    {
      "name": "large_size_length",
      "type": "text",
      "default": "10"
    },
    {
      "name": "large_size_weight",
      "type": "text",
      "default": "15"
    },
    {
      "name": "ups_username",
      "type": "text",
      "default": "15",
      "required": true
    },
    {
      "name": "ups_password",
      "type": "password",
      "default": "15",
      "required": true
    },
    {
      "name": "ups_access_number",
      "type": "text",
      "default": "15",
      "required": true
    },
    {
      "name": "ups_shipper_number",
      "type": "text",
      "required": true
      },
    {
      "name": "tracking_field",
      "type": "number",
      "default": "21947252"
    },
    {
      "name": "production_on",
      "type": "checkbox",
      "default": "no"
    }
  ]
}
